<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wx.shop.mapper.CommMapper">

    <!--  查询文件url  -->
    <select id="queryFileUrl" resultType="java.lang.String">
        select file_url from file_info
    </select>

    <insert id="insertFileInfo" parameterType="com.wx.shop.entities.pojo.FileInfoModel" >
        insert into
            file_info
        values
        (
            #{fileId, jdbcType=CHAR},
            #{fileUrl},
            #{fileSeq},
            #{fileType},
            #{isMain},
            now(),
            #{spare1},
            #{spare2},
            #{spare3}
        );
    </insert>


